{
  "name": "fibeger",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20",
    "bun": ">=1.3"
  },
  "scripts": {
    "dev": "bun --bun next dev",
    "start": "node server.js",
    "build": "bun --bun next build",
    "build:container": "bun --bun next build",
    "prisma:prepare": "cp prisma/schema.production.prisma prisma/schema.prisma || true",
    "prisma:generate": "npm run prisma:prepare && bunx --bun prisma generate",
    "prisma:db-push": "bunx --bun prisma db push",
    "lint": "eslint --max-warnings=0 .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.978.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-auth": "^4.24.13",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.30",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/twemoji": "^13.1.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.2",
    "prisma": "^5.22.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
